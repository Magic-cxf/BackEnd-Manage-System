# vscode中需要安装editorconfig for vscode  插件
root = true    #表明是最顶层文件

[*]      # 表所有文件适用
charset = utf-8       #字符集
indent_style = space     #缩进风格
indent_size = 2    #缩进大小
end_of_line = 1f   #换行类型
trim_trailing_whitespace = true  #去除行首的任意空白字符
insert_final_newline = true   #始终在行末插入一个新行


[*.md]   #md文件适用
max_line_off = true
trim_trailing_whitespace = false

